#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; // Number of test cases

    while (T--) {
        int N, M;
        cin >> N >> M; // Number of candies and number of friends

        if (N % M == 0 && N % (2 * M) == 0) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
