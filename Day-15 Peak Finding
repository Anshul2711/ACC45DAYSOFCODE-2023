#include <iostream>
using namespace std;

int main() {
    
    int t;
    cin>>t;
    while(t--)
    {
        int n,i=0;
        cin>>n;
        int arr[n];
        
        while(n--)
        {
           cin>>arr[i];
           i++;
        }
        int max=arr[0];
        
        for(n=0;n<i;n++)
        {   
            if(arr[n]> max)
            {
                max=arr[n];
            }
        }
        cout<<max<<endl;
    }
	return 0;
}
