#include <iostream>
using namespace std;

int main() {
    int N,H,z,flag=0;
    cin>>N>>H>>z;
    int T[N];
    for(int i=0;i<N;i++)
    {
        cin>>T[i];
        if(T[i]==(H-z))
        {
            flag=1;
            break;
        }
    }
    if(flag==1)
    {
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
	return 0;
}
