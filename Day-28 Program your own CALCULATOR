#include <iostream>
using namespace std;

int main() {
    float A,B;
    char C;
    cin>>A>>B>>C;
    switch(C)
    {
        case '+':
        cout<<A+B;
        break;
        case '-':
        cout<<A-B;
        break;
        case '*':
        cout<<A*B;
        break;
        case '/':
        cout<<(double)(A/B);
        break;
    }
	return 0;
}
