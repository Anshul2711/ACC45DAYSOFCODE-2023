#include <iostream>
using namespace std;

int main() 
{
    int T,flag;
    cin>>T;
    while(T>0)
    {
        int D,flag=0;
        string N;
        cin>>D;
        cin>>N;
        for(int i=0;i<D;i++)
        {
            if(N[i]=='0' || N[i]=='5')
            {
                flag=1;
            }
        }
        if(flag==0)
        {
            cout<<"NO"<<endl;
        }
        else
        {
            cout<<"YES"<<endl;
        }
        T--;
    }
    return 0;
}
